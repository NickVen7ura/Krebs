    <servlet>
        <servlet-name>ControladorEspecialistas</servlet-name>
        <servlet-class>Controlador.ControladorEspecialistas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controlador.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorRecepcionistas</servlet-name>
        <servlet-class>Controlador.ControladorRecepcionistas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorHijo</servlet-name>
        <servlet-class>Controlador.ControladorHijo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador</servlet-name>
        <servlet-class>Controlador.Controlador</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControladorCitas</servlet-name>
        <url-pattern>/ControladorCitas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorServicios</servlet-name>
        <url-pattern>/ControladorServicios</url-pattern>
