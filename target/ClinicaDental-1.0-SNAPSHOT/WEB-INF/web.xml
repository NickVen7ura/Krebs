        <servlet-name>ControladorHijo</servlet-name>
        <url-pattern>/ControladorHijo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorPadre</servlet-name>
        <url-pattern>/ControladorPadre</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
