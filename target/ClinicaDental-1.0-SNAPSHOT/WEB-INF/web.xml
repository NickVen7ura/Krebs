        <servlet-name>ControladorEspecialistas</servlet-name>
        <url-pattern>/ControladorEspecialistas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador</servlet-name>
        <url-pattern>/Controlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorRecepcionistas</servlet-name>
        <url-pattern>/ControladorRecepcionistas</url-pattern>
    </servlet-mapping>
